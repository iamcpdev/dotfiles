[user]
    name = Chaleb Pommells
        email = chaleb@iamcpdev.me
[credential]
      helper = cache --timeout=360000
[core]
      excludesfile = ~/.gitignore_global
        editor = vim
[diff]
      tool = vimdiff
[merge]
      tool = vimdiff
[difftool]
      prompt = false
[color]
      ui = true
[alias]
      lol = log --graph --decorate --pretty=tformat:'%C(blue)%h%Creset%C(yellow bold)%d%Creset %s %C(green)(%ar)%Creset %C(cyan)%an%Creset'
        st = status
          co = checkout
            br = branch
              ci = commit
                a = add
                  last = log -n 1 --
[help]
        autocorrect = 1
[push]
        default = simple
[url "https://github"]
        insteadOf = git://github
[init]
        templatedir = ~/.git_template
